{
    "name": "vscode-floyd-language-support",
    "displayName": "vscode-floyd-language-support",
    "description": "Floyd language support for VSCode.",
    "version": "0.0.2",
    "publisher": "pythooonuser",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "floyd",
            "aliases": ["Floyd", "floyd"],
            "extensions": [".floyd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "floyd",
            "scopeName": "source.floyd",
            "path": "./syntaxes/floyd.tmLanguage.json"
        }]
    }
}